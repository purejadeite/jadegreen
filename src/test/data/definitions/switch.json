{
	"id": "book",
	"sheets": [
		{
			"comment": "単一シート1件読み込み",
			"id": "main",
			"output": true,
			"target": {
			    "name": "bean"
			},
		    "cells": [
		        {"id": "lang", "value": "ja"},
		        {"id": "logicalClassName", "row": 1, "column": 3},
		        {"id": "className", "row": 2, "column": 3},
		        {"id": "package", "row": 3, "column": 3},
		        {"id": "imports", "row": 4, "column": 3, "splitter": "\n", "options": [
		            {"type": "switch", "cellId": "type", "cases": [
		                {"value": "List<LineBean>", "options": [
		                    {"type": "addvalue", "value": "java.util.ArrayList", "unique": true},
		                    {"type": "addvalue", "value": "java.util.List", "unique": true},
		                    {"type": "addvalue", "value": "com.purejadeite.generated.LineBean", "unique": true}
		                ]},
		                {"value": "Map<String, LineBean>", "options": [
		                    {"type": "addvalue", "value": "java.util.HashMap", "unique": true},
		                    {"type": "addvalue", "value": "java.util.Map", "unique": true},
		                    {"type": "addvalue", "value": "com.purejadeite.generated.LineBean", "unique": true}
		                ]}
		            ]}
		        ]},
		        {"id": "fields", "row": 8, "breakId": "fieldName", "options": [{"type": "sort", "keyId": "fieldName"}],
		            "columns": [
        		        {"id": "type", "value": "line"},
		                {"id": "logicalFieldName", "column": 2},
		                {"id": "fieldName", "column": 3},
		                {"id": "type", "column": 4},
		                {"id": "initial", "column": 5}
		            ]
		        }
		    ]
		}
	]
}
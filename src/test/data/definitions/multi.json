{
	"id": "book",
	"sheets": [
		{
			"comment": "2種類各1件①",
			"id": "main",
		    "name": "bean",
		    "cells": [
		        {"id": "logicalClassName", "row": 1, "column": 3},
		        {"id": "className", "row": 2, "column": 3},
		        {"id": "package", "row": 3, "column": 3},
		        {"id": "imports", "row": 4, "column": 3, "options": [{"class": "split", "splitter": "\n"}]},
		        {"id": "fields", "beginRow": 8, "endKeyId": "fieldName", "options": [{"class": "sort", "keyId": "fieldName"}],
		            "columns": [
		                {"id": "logicalFieldName", "column": 2},
		                {"id": "fieldName", "column": 3},
		                {"id": "method", "column": 3, "options": [{"class": "uppercamel"}]},
		                {"id": "static", "column": 3, "options": [{"class": "upperunderscore"}]},
		                {"id": "type", "column": 4},
		                {"id": "initial", "column": 5}
		            ]
		        }
		    ]
		},
		{
			"comment": "2種類各1件②",
			"id": "sub",
		    "name": "validation",
		    "cells": [
		        {"id": "logicalClassName", "row": 1, "column": 3},
		        {"id": "className", "row": 2, "column": 3},
		        {"id": "fields", "beginRow": 6, "endKeyId": "fieldName", "options": [{"class": "group", "keyId": "fieldName", "toId": "validators"}],
		            "columns": [
		                {"id": "logicalFieldName", "column": 2},
		                {"id": "fieldName", "column": 3},
		                {"id": "validationType", "column": 4, "options": [{"class": "mapping", "map": {"notnull": "NotNull"}, "lazy": true}]},
		                {"id": "arg0", "column": 5, "options": [{"class": "tonull"}]},
		                {"id": "arg1", "column": 6, "options": [{"class": "tonull"}]},
		                {"id": "arg2", "column": 7, "options": [{"class": "tonull"}]},
		                {"id": "arg3", "column": 8, "options": [{"class": "tonull"}]}
		            ]
		        }
		    ]
		}
	]
}